/* 1.simple if
 * 2.if else
 * 3.nested if
 * 4.else if ladder
 * 5.switch case
 * */
public class P005_ConditionalStatement {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("enter i = ");
		int i = sc.nextInt();
		System.out.println("enter j = ");
		int j = sc.nextInt();
		if(i > j) {
			
			
			
			
			
			
			System.out.println("i is greater than j");
		}
	}
}
