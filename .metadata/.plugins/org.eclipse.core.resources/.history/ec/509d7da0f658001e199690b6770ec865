package basic;

import java.util.HashMap;
import java.util.Map;

public class P025Map {
	public static void main(String[] args) {
		Map map = new HashMap();
		map.put(1, "c");//entry
		map.put("C++", 2);
		map.put('j', 2);
		map.put(4, "java");
		map.put(5, "python");
		map.put(6, "dart");
		map.put(6, "kotlin");
		System.out.println(map);
	}
}
