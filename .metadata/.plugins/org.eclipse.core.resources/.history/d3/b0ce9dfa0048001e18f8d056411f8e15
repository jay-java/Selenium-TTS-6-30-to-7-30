package basic;
//TypeCasting -> change data from one datatype to another.
//Two types ->1.Implicit 
//			->2.Explicit
public class P002_TypeCasting {
	public static void main(String[] args) {
		System.out.println("this is type casting code");
		double d = 12;//implicit
		System.out.println(d);
		double d1 = 1.345;
		int i = (int)d1; //explicit
		System.out.println(i);
	}
}
